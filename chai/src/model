const mongoose=require('mongoose')

const sch= new mongoose.Schema( {
    name:{
        type:String,
        required:true
    },
    email:{
        type:String,
        required:true
    },
    mobileno:{
        type:Number,
        required:true
    },
    password:{
        type:String,
        required:true
    },
    Date:{
        type:Date,
        default:Date.now

    }
})
module.exports=mongoose.model('student',sch)